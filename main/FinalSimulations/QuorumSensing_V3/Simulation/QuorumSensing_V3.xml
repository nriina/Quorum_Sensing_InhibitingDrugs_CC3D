<CompuCell3D Revision="0" Version="4.3.2">
   
   <Metadata>
      
      <!-- Basic properties simulation -->
      <NumberOfProcessors>1</NumberOfProcessors>
      <DebugOutputFrequency>10</DebugOutputFrequency>
      <!-- <NonParallelModule Name="Potts"/> -->
   </Metadata>
   
   <Potts>
      
      <!-- Basic properties of CPM (GGH) algorithm -->
      <Dimensions x="256" y="256" z="1"/>
      <Steps>100000</Steps>
      <Temperature>10.0</Temperature>
      <NeighborOrder>1</NeighborOrder>
      <Boundary_x>Periodic</Boundary_x>
      <Boundary_y>Periodic</Boundary_y>
   </Potts>
   
   <Plugin Name="CellType">
      
      <!-- Listing all cell types in the simulation -->
      <CellType TypeId="0" TypeName="Medium"/>
      <CellType TypeId="1" TypeName="Bacteria"/>
   </Plugin>
   
   <Plugin Name="Volume"/>
   
   <Plugin Name="CenterOfMass">
      
      <!-- Module tracking center of mass of each cell -->
   </Plugin>
   
   <Plugin Name="Contact">
      <!-- Specification of adhesion energies -->
      <Energy Type1="Medium" Type2="Medium">10.0</Energy>
      <Energy Type1="Medium" Type2="Bacteria">10.0</Energy>
      <Energy Type1="Bacteria" Type2="Bacteria">10.0</Energy>
      <NeighborOrder>4</NeighborOrder>
   </Plugin>
   
   <Plugin Name="Secretion">
      
      <!-- Specification of secretion properties of select cell types. -->
      <!-- You may repeat Field element for each chemical field declared in the PDE solvers -->
      <!-- Specification of secretion properties of individual cells can be done in Python -->
   </Plugin>
   
   <Steppable Type="DiffusionSolverFE">
      
      <!-- Specification of PDE solvers -->
      <DiffusionField Name="ai">
         <DiffusionData>
            <FieldName>ai</FieldName>
            <GlobalDiffusionConstant>0.1</GlobalDiffusionConstant>
            <GlobalDecayConstant>1e-05</GlobalDecayConstant>
            <!-- Additional options are: -->
            <!-- <InitialConcentrationExpression>x*y</InitialConcentrationExpression> -->
            <!-- <ConcentrationFileName>INITIAL CONCENTRATION FIELD - typically a file with path Simulation/NAME_OF_THE_FILE.txt</ConcentrationFileName> -->
            <DiffusionCoefficient CellType="Bacteria">0.1</DiffusionCoefficient>
            <DecayCoefficient CellType="Bacteria">0.0001</DecayCoefficient>
         </DiffusionData>
         <!-- <SecretionData> -->
            <!-- &lt;!&ndash; When secretion is defined inside DissufionSolverFE all secretion constants are scaled automaticaly to account for the extra calls to the diffusion step when handling large diffusion constants &ndash;&gt;  -->
            
            <!-- &lt;!&ndash; Uniform secretion Definition &ndash;&gt;  -->
            <!-- &lt;!&ndash; <Secretion Type="Bacteria">0.1</Secretion> &ndash;&gt;  -->
            <!-- &lt;!&ndash; <SecretionOnContact SecreteOnContactWith="Bacteria" Type="Bacteria">0.2</SecretionOnContact> &ndash;&gt;  -->
            <!-- &lt;!&ndash; <ConstantConcentration Type="Bacteria">0.1</ConstantConcentration> &ndash;&gt;  -->
         <!-- </SecretionData> -->
         <BoundaryConditions>
            <Plane Axis="X">
               <!-- <ConstantValue PlanePosition="Min" Value="10.0"/> -->
               <!-- <ConstantValue PlanePosition="Max" Value="5.0"/> -->
               <!-- Other options are (examples): -->
               <Periodic/>
               <!-- <ConstantDerivative PlanePosition="Min" Value="10.0"/> -->
            </Plane>
            <Plane Axis="Y">
               <!-- <ConstantDerivative PlanePosition="Min" Value="10.0"/> -->
               <!-- <ConstantDerivative PlanePosition="Max" Value="5.0"/> -->
               <!-- Other options are (examples): -->
               <Periodic/>
               <!-- <ConstantValue PlanePosition="Min" Value="10.0"/> -->
            </Plane>
         </BoundaryConditions>
      </DiffusionField>
      <DiffusionField Name="product">
         <DiffusionData>
            <FieldName>product</FieldName>
            <GlobalDiffusionConstant>0.1</GlobalDiffusionConstant>
            <GlobalDecayConstant>1e-05</GlobalDecayConstant>
            <!-- Additional options are: -->
            <!-- <InitialConcentrationExpression>x*y</InitialConcentrationExpression> -->
            <!-- <ConcentrationFileName>INITIAL CONCENTRATION FIELD - typically a file with path Simulation/NAME_OF_THE_FILE.txt</ConcentrationFileName> -->
            <DiffusionCoefficient CellType="Bacteria">0.1</DiffusionCoefficient>
            <DecayCoefficient CellType="Bacteria">0.0001</DecayCoefficient>
         </DiffusionData>
         <SecretionData>
            <!-- When secretion is defined inside DissufionSolverFE all secretion constants are scaled automaticaly to account for the extra calls to the diffusion step when handling large diffusion constants -->
            
            <!-- Uniform secretion Definition -->
            <!-- <Secretion Type="Bacteria">0.1</Secretion> -->
            <!-- <SecretionOnContact SecreteOnContactWith="Bacteria" Type="Bacteria">0.2</SecretionOnContact> -->
            <!-- <ConstantConcentration Type="Bacteria">0.1</ConstantConcentration> -->
         </SecretionData>
         <BoundaryConditions>
            <Plane Axis="X">
               <!-- <ConstantValue PlanePosition="Min" Value="10.0"/> -->
               <!-- <ConstantValue PlanePosition="Max" Value="5.0"/> -->
               <!-- Other options are (examples): -->
               <Periodic/>
               <!-- <ConstantDerivative PlanePosition="Min" Value="10.0"/> -->
            </Plane>
            <Plane Axis="Y">
               <!-- <ConstantDerivative PlanePosition="Min" Value="10.0"/> -->
               <!-- <ConstantDerivative PlanePosition="Max" Value="5.0"/> -->
               <!-- Other options are (examples): -->
               <Periodic/>
               <!-- <ConstantValue PlanePosition="Min" Value="10.0"/> -->
            </Plane>
         </BoundaryConditions>
      </DiffusionField>
      <DiffusionField Name="oxy">
         <DiffusionData>
            <FieldName>oxy</FieldName>
            <GlobalDiffusionConstant>0.1</GlobalDiffusionConstant>
            <GlobalDecayConstant>1e-05</GlobalDecayConstant>
            <!-- Additional options are: -->
            <InitialConcentrationExpression>10</InitialConcentrationExpression>
            <!-- <ConcentrationFileName>INITIAL CONCENTRATION FIELD - typically a file with path Simulation/NAME_OF_THE_FILE.txt</ConcentrationFileName> -->
            <DiffusionCoefficient CellType="Bacteria">0.1</DiffusionCoefficient>
            <DecayCoefficient CellType="Bacteria">0.0001</DecayCoefficient>
         </DiffusionData>
         <SecretionData>
            <!-- When secretion is defined inside DissufionSolverFE all secretion constants are scaled automaticaly to account for the extra calls to the diffusion step when handling large diffusion constants -->
            
            <!-- Uniform secretion Definition -->
            <!-- <Secretion Type="Bacteria">0.1</Secretion> -->
            <!-- <SecretionOnContact SecreteOnContactWith="Bacteria" Type="Bacteria">0.2</SecretionOnContact> -->
            <!-- <ConstantConcentration Type="Bacteria">0.1</ConstantConcentration> -->
         </SecretionData>
         <BoundaryConditions>
            <Plane Axis="X">
               <ConstantValue PlanePosition="Min" Value="10.0"/>
               <ConstantValue PlanePosition="Max" Value="10.0"/>
               <!-- Other options are (examples): -->
               <!-- <Periodic/> -->
               <!-- <ConstantDerivative PlanePosition="Min" Value="10.0"/> -->
            </Plane>
            <Plane Axis="Y">
            
               <ConstantValue PlanePosition="Min" Value="10.0"/>
               <ConstantValue PlanePosition="Max" Value="10.0"/>
               <!-- <ConstantDerivative PlanePosition="Min" Value="10.0"/> -->
               <!-- <ConstantDerivative PlanePosition="Max" Value="5.0"/> -->
               <!-- Other options are (examples): -->
               <!-- <Periodic/> -->
               <!-- <ConstantValue PlanePosition="Min" Value="10.0"/> -->
            </Plane>
         </BoundaryConditions>
      </DiffusionField>
   </Steppable>
   
   <Steppable Type="BlobInitializer">
      
      <!-- Initial layout of cells in the form of spherical (circular in 2D) blob -->
      <Region>
         <Center x="128" y="128" z="0"/>
         <Radius>30</Radius>
         <Gap>0</Gap>
         <Width>7</Width>
         <Types>Bacteria</Types>
      </Region>
   </Steppable>
</CompuCell3D>
